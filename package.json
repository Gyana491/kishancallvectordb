{
  "name": "kishancall-server",
  "version": "1.0.0",
  "description": "Express.js server with MongoDB Atlas and OpenAI embeddings",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "import-csv": "node import-csv.js",
    "setup-search": "node semantic-search.js",
    "test-search": "node test-search.js",
    "benchmark": "node test-search.js --benchmark",
    "performance-test": "node performance-test.js",
    "generate-embeddings": "node -e \"require('./semantic-search').generateEmbeddingsForAllDocuments().then(() => process.exit(0))\""
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "express",
    "mongodb",
    "openai",
    "embeddings",
    "vector-search"
  ],
  "author": "",
  "license": "ISC"
}
